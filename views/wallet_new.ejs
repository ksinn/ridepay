<% layout('layout') -%>

<div class="inner cover">
    <form action="/wallet/new" method="post" class="form-horizontal">
        <div class="form-group">
            <label class="col-sm-2 control-label">Ownr name: </label>
            <div class="col-sm-10">
                <input class="form-control" name="ownr[name]" type="text">
            </div>
        </div>
        <div class="form-group">
            <label class="col-sm-2 control-label">Ownr family: </label>
            <div class="col-sm-10">
                <input class="form-control" name="ownr[family]" type="text">
            </div>
        </div>
        <div class="form-group">
            <button class="btn btn-default">
                Create
            </button>
        </div>
    </form>
</div>